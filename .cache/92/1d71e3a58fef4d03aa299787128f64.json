n    font-size: 15px;\r\n    background-color:white;\r\n\r\n }\r\n\r\n .login-btn:hover{\r\n    border: none;\r\n    color: #ecaf22;\r\n    cursor: pointer;\r\n    \r\n }\r\n/* Restaurants Container */\r\n.res-container {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n    gap: 20px;\r\n    margin-top: 15px;\r\n    padding: 20px;\r\n}\r\n\r\n/* Restaurant Card */\r\n.res-card {\r\n    background-color: #ffffff;  /* White background */\r\n    color: #333;  /* Dark text */\r\n    width: 200px;\r\n    padding: 15px;\r\n    border-radius: 8px;\r\n    text-align: center;\r\n    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1); /* Subtle shadow */\r\n    transition: transform 0.2s ease, border 0.2s ease;\r\n}\r\n\r\n.res-card:hover {\r\n    cursor: pointer;\r\n    transform: translateY(-5px);\r\n    border: 1px solid #ddd;\r\n}\r\n\r\n/* Restaurant Card Content */\r\n.res-card h2 {\r\n    font-size: 20px;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.res-card .rating {\r\n    font-size: 16px;\r\n    color: #ffcc00; /* Yellow rating */\r\n    font-weight: bold;\r\n}\r\n\r\n.res-card .delivery-time {\r\n    font-size: 14px;\r\n    color: #888; /* Subtle gray for delivery time */\r\n}\r\n/* Filter Button Container */\r\n.filter-buttons-container {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n    padding: 10px;\r\n  }\r\n  \r\n  /* Search Input */\r\n  .search-container input {\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    width: 200px; /* Adjust width of the input field */\r\n    border-radius: 5px;\r\n    border: 1px solid #ccc;\r\n    height: 15px;\r\n  }\r\n  \r\n  /* Search Button (Wider Button) */\r\n  .search-btn {\r\n    width: 150px; /* Increase the width of the button */\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    background-color: #ECC622;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n  \r\n  .search-btn:hover {\r\n    background-color: #ECC622;\r\n  }\r\n  \r\n  /* General Filter Button Style */\r\n  .filter-btn {\r\n    padding: 10px 20px;\r\n    background-color: #ece222;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n  \r\n  .filter-btn:hover {\r\n    background-color: #ecaf22;\r\n  }\r\n\r\n  .res-filter-btn-container{\r\n        display: flex;\r\n        -webkit-box-align: center;\r\n        align-items: center;\r\n        justify-content: center;\r\n        height: 44px;\r\n        width: 100%;\r\n        cursor: pointer;\r\n        margin-bottom: 4px;\r\n        margin-top: 4px;\r\n        /* background-color: rgb(255, 255, 255); */\r\n    \r\n    \r\n  }\r\n  .res-filter-btn-container div{\r\n    list-style: none;\r\n    padding: 15px;\r\n    text-align: center;\r\n    width: auto;\r\n    border-radius: 25px;\r\n    border: 1px solid #333;\r\n    width: 150px;\r\n    margin: 5px;\r\n  }\r\n\r\n  .res-filter-btn-container div:hover{\r\n    background-color: #ecaf22;\r\n    color: whitesmoke;\r\n  }\r\n\r\n  \r\n  "},"lineCount":206}},"error":null,"hash":"24059438a1582fbde5bff24b02b4ab70","cacheData":{"env":{}}}