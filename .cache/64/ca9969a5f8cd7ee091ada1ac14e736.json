ckground-color: #fff;\r\n  }\r\n  \r\n  /* Restaurant name styling */\r\n  .res-name {\r\n    font-size: 2rem;\r\n    font-weight: bold;\r\n    margin-bottom: 20px;\r\n    text-align: center;\r\n    color: #333;\r\n  }\r\n  \r\n  /* Search input field styling */\r\n  input[type=\"text\"],\r\n  input[type=\"search\"] {\r\n    width: 100%; /* Ensures the search input takes the full width */\r\n    padding: 10px;\r\n    margin: 10px 0 20px;\r\n    border: none; /* Removed the border */\r\n    border-radius: 8px;\r\n    font-size: 1rem;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  /* Styling for the container holding menu cards */\r\n  .res-menu-card {\r\n    display: flex;\r\n    flex-direction: column; /* Column layout for menu items */\r\n    gap: 20px;\r\n    width: 100%; /* Ensure the menu cards take up the full width of the container */\r\n    padding: 0 20px; /* Ensure some padding around the menu items */\r\n  }\r\n  \r\n  /* --- ItemCard Styling --- */\r\n  .item-card {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    padding: 20px;\r\n    width: 100%; /* Make item card take full width */\r\n    max-width: 500px; /* Limit the maximum width of item cards */\r\n    border-radius: 8px;\r\n    background-color: #fff;\r\n    box-shadow: 0 6px 10px rgba(0, 0, 0, 0.15); /* Box shadow for depth */\r\n    transition: transform 0.2s ease, box-shadow 0.2s ease;\r\n  }\r\n  \r\n  .item-card:hover {\r\n    transform: translateY(-5px);\r\n    box-shadow: 0 10px 15px rgba(0, 0, 0, 0.2); /* Shadow on hover */\r\n  }\r\n  \r\n  /* Item name styling */\r\n  .item-card h4 {\r\n    font-size: 1.5rem;\r\n    font-weight: bold;\r\n    color: #333;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  /* Item price styling */\r\n  .item-card p {\r\n    font-size: 1rem;\r\n    color: #555;\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  /* Add to Cart button */\r\n  .item-card button {\r\n    padding: 10px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    font-size: 1rem;\r\n    cursor: pointer;\r\n    text-align: center;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n  \r\n  .item-card button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  \r\n  /* Image inside item card (optional, if applicable) */\r\n  .item-card img {\r\n    width: 100%;\r\n    height: auto;\r\n    border-radius: 8px;\r\n    margin-bottom: 10px;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  }\r\n  .item-heading {\r\n    display: flex;\r\n    justify-content: space-between; /* Pushes items to the left and right */\r\n    align-items: center; /* Vertically aligns items */\r\n    width: 100%; /* Ensures the flex container takes full width */\r\n  }\r\n  .price-footer{\r\n    width: 100%;\r\n    position: fixed;\r\n    bottom:0 ;\r\n    background-color: #ECC622;\r\n    z-index: 100px;\r\n    text-align: center;\r\n  }\r\n  "},"lineCount":null}},"error":null,"hash":"9dc6b6f0172d1a06970aad8e82de3484","cacheData":{"env":{}}}